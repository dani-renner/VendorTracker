@{
  Layout = "_Layout";
}
@using VendorTracker.Models;
<h1>Orders</h1>
@if (Model.Count == 0)
{
  <p>There are no orders in the list.</p>
}
<ul>
  @foreach (Order order in Model)
  {
    <li><a href='/orders/@order.Id'>@order.OrderDetails</a></li>
  }
</ul>
<a href='/orders/new'>Add a New Order</a>